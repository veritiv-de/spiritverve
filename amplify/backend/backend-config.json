{
  "function": {
    "helloWorld": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "api": {
    "helloworldapi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "helloWorld",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "hosting": {
    "amplifyhosting": {
      "service": "amplifyhosting",
      "type": "manual"
    }
  },
  "custom": {
    "apiGatewayUrl": {
      "service": "custom",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "helloworldapi",
          "attributes": [
            "RootUrl"
          ]
        }
      ]
    }
  }
}